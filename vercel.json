{
  "rewrites": [
    {
      "source": "/",
      "destination": "/login.html"
    },
    {
      "source": "/api/(.*)",
      "destination": "https://disc-seginfo-final-back.vercel.app/$1"
    }
  ],
  "redirects": [
    {
      "source": "/index.html",
      "destination": "/login.html",
      "statusCode": 302
    }
  ]
}